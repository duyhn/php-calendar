{
    "name": "app/calendar",
    "description": "This is a simple calendar",
    "minimum-stability": "dev",
    "require": {
        "php": "^7.0",
        "vlucas/phpdotenv": "^2.5@dev",
        "twig/twig": "2.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Config\\": "config/",
            "App\\": "app/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0@dev",
        "phpmd/phpmd": "dev-master",
        "squizlabs/php_codesniffer": "^3.0@dev",
        "mockery/mockery": "^1.0@dev"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
